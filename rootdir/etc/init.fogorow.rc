# FPSGO FBT Game
on boot
    # allow af_noise
    chown system system /sys/class/actuatordrv_main_af/MAINAF/af_noise
    chmod 0664 /sys/class/actuatordrv_main_af/MAINAF/af_noise

on post-fs-data
    chown system system /sys/devices/platform/11015000.spi5/spi_master/spi5/spi5.0/irq
    chmod 0660 /sys/devices/platform/11015000.spi5/spi_master/spi5/spi5.0/irq
    chown system system /sys/devices/platform/11015000.spi5/spi_master/spi5/spi5.0/wakeup_enable
    chmod 0660 /sys/devices/platform/11015000.spi5/spi_master/spi5/spi5.0/wakeup_enable
    chown system system /sys/devices/platform/11015000.spi5/spi_master/spi5/spi5.0/hw_reset
    chmod 0660 /sys/devices/platform/11015000.spi5/spi_master/spi5/spi5.0/hw_reset
    chown system system /sys/devices/platform/11015000.spi5/spi_master/spi5/spi5.0/clk_enable
    chmod 0660 /sys/devices/platform/11015000.spi5/spi_master/spi5/spi5.0/clk_enable
    chown system system /sys/devices/platform/11015000.spi5/spi_master/spi5/spi5.0/compatible_all
    chmod 0660 /sys/devices/platform/11015000.spi5/spi_master/spi5/spi5.0/compatible_all

    mkdir /data/local/tmp 0777 system system

    chmod 0660 /dev/goodix_fp
    chown system system /dev/goodix_fp

    chmod 0777 /dev/pn553
    chown nfc nfc /dev/pn553

    chmod 0664 /sys/devices/platform/charger/demo_mode_limit
    chown system system /sys/devices/platform/charger/demo_mode_limit

    chmod 0664 /sys/devices/platform/charger/turbo_power_mode
    chown system system /sys/devices/platform/charger/turbo_power_mode

    chmod 0664 /sys/class/power_supply/battery/temperature
    chown system system /sys/class/power_supply/battery/temperature

on init
#add vibrator_duration
    chown system system /sys/devices/platform/regulator_vibrator/vibrator_duration
